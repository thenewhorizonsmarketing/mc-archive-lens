{
  "name": "3d-clue-board-kiosk",
  "productName": "3D Clue Board Kiosk",
  "description": "3D Interactive Kiosk Interface for MC Museum & Archives",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "vite",
    "dev:electron": "concurrently \"npm run dev\" \"wait-on http://localhost:5173 && cross-env NODE_ENV=development electron .\"",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "build:prod": "cross-env NODE_ENV=production vite build",
    "build:electron": "npm run build:prod && cross-env NODE_ENV=production vite build --config electron.vite.config.ts",
    "build:electron:main": "vite build --config electron.vite.config.ts",
    "prebuild:production": "npm run optimize:assets && npm run validate:assets",
    "build:production": "npm run prebuild:production && npm run build:electron",
    "package:win": "npm run build:production && electron-builder --win --x64",
    "package:win:portable": "npm run build:production && electron-builder --win portable --x64",
    "package:kiosk": "cross-env KIOSK_MODE=true npm run package:win",
    "package:all": "npm run build:production && electron-builder --win --x64",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "deploy": "node scripts/deploy.js",
    "deploy:check": "node scripts/deploy.js --check",
    "deploy:checklist": "node scripts/deployment-checklist.js",
    "deploy:verify": "npm run deploy:checklist && npm run build && npm run test:run",
    "type-check": "tsc --noEmit",
    "deploy:build": "node scripts/deploy.js --build",
    "deploy:db": "node scripts/deploy.js --db-init",
    "start:kiosk": "vite preview --host 0.0.0.0 --port 3000",
    "optimize:assets": "node scripts/optimize-assets.js",
    "optimize:textures:ktx2": "node scripts/compress-textures-ktx2.js",
    "optimize:geometry:draco": "node scripts/compress-geometry-draco.js",
    "optimize:all": "npm run optimize:geometry:draco && npm run optimize:textures:ktx2 && npm run optimize:assets",
    "validate:assets": "node scripts/validate-assets.cjs",
    "validate:boot": "node scripts/validate-boot-performance.js",
    "validate:hit-targets": "node scripts/validate-hit-targets.js",
    "validate:offline": "node scripts/validate-offline-operation.js",
    "validate:fps": "node scripts/validate-fps-performance.js",
    "validate:touch-targets": "node scripts/validate-touch-targets.js",
    "validate:visual-feedback": "node scripts/validate-visual-feedback.js",
    "test:all": "node scripts/run-all-tests.cjs"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-aspect-ratio": "^1.1.7",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-context-menu": "^2.2.15",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-hover-card": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-menubar": "^1.1.15",
    "@radix-ui/react-navigation-menu": "^1.2.13",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.14",
    "@radix-ui/react-toggle": "^1.1.9",
    "@radix-ui/react-toggle-group": "^1.1.10",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@react-three/drei": "^9.108.3",
    "@react-three/fiber": "^8.16.8",
    "@react-three/postprocessing": "^3.0.4",
    "@tanstack/react-query": "^5.83.0",
    "@types/papaparse": "^5.5.0",
    "@types/react-window": "^1.8.8",
    "@types/sql.js": "^1.4.9",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^3.6.0",
    "embla-carousel-react": "^8.6.0",
    "framer-motion": "^12.23.24",
    "input-otp": "^1.4.2",
    "jszip": "^3.10.1",
    "lucide-react": "^0.462.0",
    "next-themes": "^0.3.0",
    "papaparse": "^5.5.3",
    "pdfjs-dist": "^5.4.394",
    "react": "^18.3.1",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^6.0.0",
    "react-hook-form": "^7.61.1",
    "react-pdf": "^10.2.0",
    "react-resizable-panels": "^2.1.9",
    "react-router-dom": "^6.30.1",
    "react-window": "^2.2.3",
    "react-window-infinite-loader": "^2.0.0",
    "recharts": "^2.15.4",
    "sonner": "^1.7.4",
    "sql.js": "^1.13.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "three": "^0.181.0",
    "vaul": "^0.9.9",
    "zod": "^3.25.76",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.16.5",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@types/three": "^0.181.0",
    "@vitejs/plugin-react-swc": "^3.11.0",
    "@vitest/ui": "^4.0.7",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^39.1.1",
    "electron-builder": "^26.0.12",
    "eslint": "^9.32.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^15.15.0",
    "gltf-pipeline": "^4.3.0",
    "jsdom": "^27.0.1",
    "lovable-tagger": "^1.1.11",
    "playwright": "^1.56.1",
    "postcss": "^8.5.6",
    "sharp": "^0.34.5",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0",
    "vite": "^5.4.19",
    "vitest": "^4.0.7",
    "wait-on": "^9.0.3"
  }
}
